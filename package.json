{
  "name": "gadmwaye",
  "version": "0.0.1",
  "description": "A modern rewrite of github-secret-keeper",
  "main": "bin/index.js",
  "scripts": {
    "lint": "npx eslint --fix --cache src",
    "build": "npx babel src --out-dir bin",
    "work": "npx babel src -s --watch --verbose --out-dir bin",
    "start": "micro",
    "dev": "micro-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leny/gadmwaye.git"
  },
  "author": "leny <info@flatland.be>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leny/gadmwaye/issues"
  },
  "homepage": "https://github.com/leny/gadmwaye#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@becode/eslint-config": "^0.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-unicorn": "^8.0.2",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "micro-dev": "^3.0.0",
    "prettier": "^1.17.1"
  },
  "lint-staged": {
    "*.json": [
      "npx prettier --write",
      "git add"
    ],
    "*.js": [
      "npx eslint --fix --cache",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "dependencies": {
    "core-js": "^3.0.1",
    "got": "^9.6.0",
    "micro": "^9.3.4",
    "parseurl": "^1.3.3",
    "qs": "^6.7.0"
  }
}
